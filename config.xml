<?xml version='1.0' encoding='utf-8'?>
<widget id="io.dctdevlop.hellosyrus" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Syrus Bt Example</name>
    <description>
        A sample Apache Cordova application that sends and response commands from syrus Bluetooth Devices
    </description>
    <author email="gabriel.bejarano@digitalcomtech.com" href="http://support/digitalcomtech.com">
        DCT Developers
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="loadUrlTimeoutValue" value="700000" />
    <plugin name="cordova-plugin-ble-central" spec="~1.1.4" />
</widget>
